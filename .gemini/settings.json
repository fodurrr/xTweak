{
  "mcpServers": {
    "tidewave": {
      "httpUrl": "http://localhost:4000/tidewave/mcp",
      "description": "MCP server for Elixir Tidewave."
    },
    "ash_ai": {
      "httpUrl": [
        "http://localhost:4000/ash_ai/mcp"
      ],
      "description": "MCP server for Ash AI."
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {}
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {
        "DEBUG": "pw:mcp"
      }
    }
  },
  "ui": {
    "theme": "Default"
  }
}